<p id="notice"><%= notice %></p>

<h1>Instructors</h1>
<%= will_paginate @instructors %>
<table style="min-width:100%">
  <thead>
    <tr>
      <th>Last Name</th>
      <th>First Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instructors.each do |instructor| %>
      <tr>
        <td><%= instructor.lastname %></td>
        <td><%= instructor.firstname %></td>
        <td><%= instructor.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @instructors %>
<br>

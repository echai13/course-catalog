<p id="notice"><%= notice %></p>

<h1>Courses</h1>
<%= will_paginate @courses %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Course Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.name %></td>
        <td><%= course.description %></td>
        <td><%= course.coursecode %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @courses %>

<br>
